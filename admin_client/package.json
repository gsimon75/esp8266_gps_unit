  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --inline-vue --mode development",
    "lint": "vue-cli-service lint",
    "builder": "vue-cli-service build --inline-vue --mode development --watch --stdin",
    "buildprod": "vue-cli-service build --inline-vue",
    "capacitor:build": "vue-cli-service capacitor:build",
    "capacitor:serve": "vue-cli-service capacitor:serve",
    "postcapacitor:build": "$ANDROID_SDK_ROOT/build-tools/29.0.2/apksigner sign --ks android/apk_signer.keystore --ks-pass pass:android dist/app-release.apk && adb install -r dist/app-release.apk",
